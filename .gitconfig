[user]
	name = Enming Xie
	email = xxx
[color]
	ui = auto
[alias]
	st = status
	pl = pull --ff-only
	ps = push
	co = checkout
	ci = commit
	df = difftool
	cp = cherry-pick
	ca = commit --amend
	br = branch
	l = log --stat
	last = log -1
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	up = push origin
	lf = log --follow -p
	aa = ls-files --modified
	su = status -uno
	cm = commit -m
	uf = push origin -f
	cl = clean -fdx
	fe = fetch --prune
	cc = checkout 5.1_SP0/main
[push]
	default = simple
[core]
	editor = vim
	pager = 
[diff]
	tool = meld
[difftool]
	prompt = false
[merge]
	tool = meld
[init]
	templatedir = ~/.git_template
